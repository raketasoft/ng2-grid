{
  "name": "raketasoft-ng2-grid",
  "version": "1.0.0-beta.9",
  "description": "Lightweight data grid component for Angular 2",
  "scripts": {
    "clean": "gulp clean",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "server:start": "node demo/server.js",
    "start": "tsc && concurrently \"tsc -w\" \"npm run serve\" ",
    "e2e": "tsc && concurrently \"npm run serve:e2e\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./src/**/*[^.d$].ts ./demo/**/*[^.d$].ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test:once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Branimir Borisov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raketasoft/ng2-grid.git"
  },
  "keywords": [
    "angular2",
    "ng2",
    "grid",
    "angular2-grid",
    "ng2-grid",
    "datatable",
    "angular2-datatable",
    "ng2-datatable"
  ],
  "dependencies": {
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@types/lodash": "4.14.71",
    "core-js": "^2.5.0",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.8",
    "rxjs": "~5.5.10",
    "systemjs": "0.20.17",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@types/del": "^2.2.32",
    "@types/gulp": "^4.0.0",
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.19",
    "@types/selenium-webdriver": "2.53.33",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "~3.2.1",
    "http-server": "^0.10.0",
    "httpdispatcher": "^2.0.1",
    "jasmine-core": "~2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.5.4",
    "tslint": "^5.5.0",
    "typescript": "2.6.2"
  }
}
